{
  "tags": {
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["./src", "./tests", "./index.js", "./bin", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown", "node_modules/better-docs/typedef-import"],
  "opts": {
    "encoding": "utf8",
    "template": "node_modules/clean-jsdoc-theme",
    "destination": "docs/",
    "readme": "README.md",
    "recurse": true,
    "theme_opts": {
      "default_theme": "dark"
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  },
  "theme_opts": {
    "homepageTitle": "Sans prise de bot code documentation",
    "title": "Sans prise de bot code documentation",
    "menu": [
      {
        "title": "Github",
        "link": "https://github.com/MrStanDu33/SansPriseDeBot",
        "target": "_blank"
      }
    ],
    "sections": [
      "Classes",
      "Modules",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Tutorials",
      "Interfaces",
      "Global"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
