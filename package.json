{
  "name": "sansprisedebot",
  "version": "1.0.0",
  "description": "Discord Bot that help new users to select the rights roles",
  "homepage": "https://github.com/MrStanDu33/SansPriseDeBot/project#readme",
  "bugs": {
    "url": "https://github.com/MrStanDu33/SansPriseDeBot/issues",
    "email": "contact@daniels-roth-stan.fr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MrStanDu33/SansPriseDeBot"
  },
  "engines": {
    "node": ">=20.11.1"
  },
  "engineStrict": true,
  "main": "./dist/index.js",
  "type": "module",
  "author": "DANIELS-ROTH Stan <contact@daniels-roth-stan.fr> (https://daniels-roth-stan.fr/)",
  "license": "GPL-3.0-or-later",
  "private": "true",
  "scripts": {
    "lint": "eslint . --ext .js --ext .ts",
    "test": "node --experimental-vm-modules --no-warnings ./node_modules/.bin/jest --forceExit",
    "test:debug": "node --inspect-brk --experimental-vm-modules --no-warnings ./node_modules/.bin/jest --runInBand --watch",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "start:prod": "node --experimental-loader=extensionless --experimental-vm-modules --experimental-specifier-resolution=node dist/index.js",
    "build:prod": "rimraf dist && tsc && tsc-alias",
    "dev": "tsx .",
    "start": "npm run build:prod && npm run start:prod",
    "seed:roles": "tsx ./bin/seed/roles.js",
    "seed:actions": "tsx ./bin/seed/actions.ts ./src/Db/DecisionsTrees/FormationRolesDecisionsTree.json",
    "docs": "node_modules/.bin/jsdoc -c jsdoc.json",
    "docs:watch": "npx nodemon --exec 'npm run docs' --watch . --ignore ./docs/"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^10.1.0",
    "@sequelize/core": "^7.0.0-alpha.43",
    "@sequelize/mysql": "^7.0.0-alpha.43",
    "@types/i18n": "^0.13.12",
    "@types/node": "^22.7.6",
    "chalk": "5.3.0",
    "cli-spinners": "^2.9.2",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "extensionless": "^1.9.9",
    "i18n": "^0.15.1",
    "json-refs": "^3.0.15",
    "mysql2": "^3.11.3",
    "node-cron": "^3.0.3",
    "ora": "^6.3.1",
    "source-map-support": "^0.5.21",
    "stack-trace": "*",
    "tsx": "^4.19.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.8",
    "@babel/plugin-syntax-import-assertions": "^7.25.7",
    "@babel/preset-env": "^7.25.8",
    "@dotenv/cli": "^2.2.2",
    "@jest/globals": "29.7.0",
    "@types/jest": "^29.5.13",
    "@types/node-cron": "^3.0.11",
    "@types/source-map-support": "^0.5.10",
    "@types/stack-trace": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
